<{{#if check--IsLabelWrapped}}span{{else if check-label--type}}{{check-label--type}}{{else}}label{{/if}} class="{{pfv}}check__label
  {{#if check-label--IsDisabled}}
    pf-m-disabled
  {{/if}}
  {{#if check-label--modifier}}
    {{check-label--modifier}}
  {{/if}}"
  {{#unless check--IsLabelWrapped}}
    for="{{check--id}}-input"
  {{/unless}}
  {{#if check-label--attribute}}
    {{{check-label--attribute}}}
  {{/if}}>
  {{#if check-label--text}}
    {{check-label--text}}
  {{else if @partial-block}}
    {{> @partial-block}}
  {{/if}}
  {{#if check--IsRequired}}
    {{> check-label-required}}
  {{/if}}
</{{#if check--IsLabelWrapped}}span{{else if check-label--type}}{{check-label--type}}{{else}}label{{/if}}>
