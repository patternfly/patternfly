{{#> backdrop}}
  {{#> about-modal-box pf-c-about-modal-box__label="aboutModalTitle"}}
    {{#> about-modal-box-hero}}{{/about-modal-box-hero}}
    {{#> about-modal-box-brand}}
      {{#> about-modal-box-brand-image url="/assets/images/pf_mini_logo_white.svg"}}{{/about-modal-box-brand-image}}
    {{/about-modal-box-brand}}
    {{#> about-modal-box-closer}}
      {{#> button btnClass="pf-m-action pf-m-close"}}
        <i class="fas fa-times"></i> 
      {{/button}}
    {{/about-modal-box-closer}}
    {{#> about-modal-box-strapline}}
      {{#> title titleType="h2" pf-c-title--Modifier="pf-m-md"}}
          Trademark and copyright information here
      {{/title}}
    {{/about-modal-box-strapline}}
    {{#> about-modal-box-header}}
      {{#> title titleType="h1" pf-c-title--Modifier="pf-m-xxxxl" pf-c-title--ID="aboutModalTitle"}}
          Red Hat OpenShift Container Platform
      {{/title}}
    {{/about-modal-box-header}}
    {{#> about-modal-box-logo}}
      {{#> about-modal-box-logo-image}}
        <img src="/assets/images/pf_logo.svg" alt="Product Logo">
      {{/about-modal-box-logo-image}}
    {{/about-modal-box-logo}}
    {{#> about-modal-box-content}}
      <dl>
        <dt>CFME Version</dt>
        <dd>5.5.3.4.20102789036450</dd>

        <dt>Cloudforms Version</dt>
        <dd>4.1</dd>

        <dt>Server Name</dt>
        <dd>40DemoMaster</dd>

        <dt>User Name</dt>
        <dd>Administrator</dd>

        <dt>User Role</dt>
        <dd>EvmRole-super_administrator</dd>

        <dt>Browser Version</dt>
        <dd>601.2</dd>

        <dt>Browser OS</dt>
        <dd>Mac</dd>
      </dl>
    {{/about-modal-box-content}}
  {{/about-modal-box}}
{{/backdrop}}
