{{#> about-modal-box pf-c-about-modal-box-modifier="" pf-c-about-modal-box__label="aboutModalTitle" }}
  {{#> about-modal-box-brand }}
    {{#> about-modal-box-brand-image }}
/assets/images/pf_mini_logo_white.svg
    {{/ about-modal-box-brand-image }}
  {{/ about-modal-box-brand }}
  {{#> about-modal-box-closer }}
    {{#> button btnClass="pf-m-tertiary"}}
      <i class="fas fa-times"></i> 
    {{/button}}
  {{/ about-modal-box-closer }}
  {{#> about-modal-box-header }}
    {{#> title titleType="h2" pf-c-title--Modifier="pf-m-md "}}
        Trademark and copyright information here
    {{/title}}
    {{#> title titleType="h1" pf-c-title--Modifier="pf-m-xxxxl" pf-c-title--ID="aboutModalTitle"}}
        Product Name
    {{/title}}
  {{/ about-modal-box-header }}
  {{#> about-modal-box-hero }}
    {{#> about-modal-box-hero-image }}
/assets/images/pfbg_992.jpg
    {{/ about-modal-box-hero-image }}
  {{/ about-modal-box-hero }}
  {{#> about-modal-box-logo }}
    {{#> about-modal-box-logo-background }}
      <img src="/assets/images/pf_logo.svg" alt="Product Logo">
    {{/ about-modal-box-logo-background }}
  {{/ about-modal-box-logo }}
  {{#> about-modal-box-content }}
    content
  {{/ about-modal-box-content }}

{{/ about-modal-box}}
