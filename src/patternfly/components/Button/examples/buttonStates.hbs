<div class="pf-l-level">
<div class="pf-l-stack">
    {{#> button btnClass="pf-is-primary"}}
        Primary
    {{/button}}
    {{#> button btnClass="pf-is-primary pf-is-focus"}}
        Primary Focus
    {{/button}}
    {{#> button btnClass="pf-is-primary pf-is-active" btnAttributes="ariaPressed=\"true\""}}
        Primary Active
    {{/button}}
    {{#> button btnClass="pf-is-primary pf-is-disabled" btnAttributes="disabled=\"true\" tabIndex=\"-1\" ariaDisabled=\"true\"" }}
        Primary Disabled
    {{/button}}
</div>
