<{{#if divider--type}}{{divider--type}}{{else}}hr{{/if}} class="{{pfv}}divider
  {{setModifiers
    divider--IsVertical='pf-m-vertical'
    divider--modifier=divider--modifier
  }}"
{{#if divider--attribute}}
  {{divider--attribute}}
{{/if}}
{{#if divider--type}}
  {{#if divider--role}}
    role={{divider--role}}
  {{else}}
    role="separator"
  {{/if}}
{{/if}}>
{{#if divider--type}}
  </{{divider--type}}>
{{/if}}
