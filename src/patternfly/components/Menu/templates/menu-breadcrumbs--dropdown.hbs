{{#> breadcrumb-item}}
  {{> breadcrumb-item-divider}}
  {{#> breadcrumb-dropdown}}
    {{#> menu-toggle menu-toggle--IsPlain=true}}
      {{#> badge badge--modifier="pf-m-read"}}
        {{menu-toggle--badge--text}} <i class="fas fa-caret-down" aria-hidden="true"></i>
      {{/badge}}
    {{/menu-toggle}}
    {{#> menu newcontext}}
      {{#> menu-content}}
        {{#> menu-list}}
          {{> @partial-block}}
        {{/menu-list}}
      {{/menu-content}}
    {{/menu}}
  {{/breadcrumb-dropdown}}
{{/breadcrumb-item}}
