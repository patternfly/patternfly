<{{# if menu-item--IsLink}}a{{else}}button{{/if}} class="pf-c-menu__item{{#if menu-item--modifier}} {{menu-item--modifier}}{{/if}}"
  {{#if menu-item--IsLink}}
    href="#"
  {{else}}
    type="button"
  {{/if}}
  {{#if menu-item--attribute}}
    {{{menu-item--attribute}}}
  {{/if}}
  {{#if menu-item--data-target}}
    data-target="{{{menu-item--data-target}}}"
  {{/if}}
  {{!-- aria-expanded value can still be passed manually --}}
  {{#if menu-list-item--IsExpandable}}
    {{#if menu-item--IsExpanded}}
      aria-expanded="false"
    {{else}}
      aria-expanded="false"
    {{/if}}
  {{/if}}>
  {{> @partial-block}}
</{{# if menu-item--IsLink}}a{{else}}button{{/if}}>
