          {{> button-icon button-icon--type="times-circle"}}
        {{/button}}
      {{/chip}}
      {{#> chip}}
        {{#> chip-text chip-text--attribute=(concat 'id="' chip-group--id 'chip_three_toolbar_collapsed"')}}
