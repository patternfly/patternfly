{{#> chip-group chip--id="select-collapsed"}}
  {{#> chip}}
    {{#> chip-text chip-text--attribute=(concat 'id="' chip--id 'chip_one"')}}
      Chip One
    {{/chip-text}}
    {{#> button button--modifier="pf-m-plain" button--HasTimesCircleIcon="true" button--attribute=(concat 'aria-labelledby="' chip--id 'remove_chip_one ' chip--id 'chip_one" aria-label="Remove" id="' chip--id 'remove_chip_one"')}}
    {{/button}}
  {{/chip}}
  {{#> chip}}
    {{#> chip-text chip-text--attribute=(concat 'id="' chip--id 'chip_two"')}}
      Chip Two
    {{/chip-text}}
    {{#> button button--modifier="pf-m-plain" button--HasTimesCircleIcon="true" button--attribute=(concat 'aria-labelledby="' chip--id 'remove_chip_two ' chip--id 'chip_two" aria-label="Remove" id="' chip--id 'remove_chip_two"')}}
    {{/button}}
  {{/chip}}
  {{#> chip}}
    {{#> chip-text chip-text--attribute=(concat 'id="' chip--id 'chip_three"')}}
      Chip Three
    {{/chip-text}}
    {{#> button button--modifier="pf-m-plain" button--HasTimesCircleIcon="true" button--attribute=(concat 'aria-labelledby="' chip--id 'remove_chip_three ' chip--id 'chip_three" aria-label="Remove" id="' chip--id 'remove_chip_three"')}}
    {{/button}}
  {{/chip}}
	{{#> chip chip--modifier="pf-m-overflow"}}
  	{{#> button button--modifier="pf-m-plain"}}
    	{{#> chip-text}}
      	2 more
    	{{/chip-text}}
  	{{/button}}
	{{/chip}}
{{/chip-group}}
