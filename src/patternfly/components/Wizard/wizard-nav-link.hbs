<{{#if wizard-nav-link--IsLink}}a href="#"{{else}}button{{/if}} class="pf-c-wizard__nav-link
  {{~#if wizard-nav-link--IsCurrent}}
    pf-m-current
  {{/if}}
  {{~#if wizard-nav-link--IsDisabled}}
    {{~#if wizard-nav-link--IsLink}}
      pf-m-disabled
    {{/if}}
  {{/if}}
  {{~#if wizard-nav-link--modifier}}
    {{wizard-nav-link--modifier}}
  {{/if}}"
  {{#if wizard-nav-link--IsCurrentPage}}
    aria-current="page"
  {{/if}}
  {{#if wizard-nav-link--IsDisabled}}
    {{#if wizard-nav-link--IsLink}}
      aria-disabled="true"
      tabindex="-1"
    {{else}}
      disabled
    {{/if}}
  {{/if}}
  {{#if wizard-nav-item--IsExpandable}}
    {{#if wizard-nav-item--IsExpanded}}
      aria-expanded="true"
    {{else}}
      aria-expanded="false"
    {{/if}}
  {{/if}}
  {{#if wizard-nav-link--attribute}}
    {{{wizard-nav-link--attribute}}}
  {{/if}}>
  {{#if wizard-nav-item--IsExpandable}}
    {{#> wizard-nav-link-text}}
      {{> @partial-block}}
    {{/wizard-nav-link-text}}
    {{#> wizard-nav-link-toggle}}
      {{> wizard-nav-link-toggle-icon}}
    {{/wizard-nav-link-toggle}}
  {{else}}
    {{> @partial-block}}
  {{/if}}
</{{#if wizard-nav-link--IsLink}}a{{else}}button{{/if}}>
