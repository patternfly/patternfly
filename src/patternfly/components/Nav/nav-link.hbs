<{{#if nav-item--expandable}}button{{else}}a href="{{nav-link--href}}"{{/if}} class="{{pfv}}nav__link{{#if nav-link--current}} pf-m-current{{/if}}{{#if nav-link--modifier}} {{nav-link--modifier}}{{/if}}"
  {{#if nav-link--attribute}}
    {{{nav-link--attribute}}}
  {{/if}}
  {{#if nav-link--current}}
    aria-current="page"
  {{/if}}
  {{#if nav-item--IsFlyout}}
    aria-haspopup="true"
  {{/if}}
  {{#ifAny nav-item--IsFlyout nav-item--IsDrilldown nav-item--expandable}}
    {{#if nav-item--expanded}}
      aria-expanded="true"
    {{else}}
      aria-expanded="false"
    {{/if}}
  {{/ifAny}}>
  {{#if nav-item--IsDrillup}}
    {{> nav-toggle nav-toggle--modifier="pf-m-start"}}
  {{/if}}
  {{#if nav-item--HasTextWrapper}}
    {{#> nav-link-text}}
      {{> @partial-block}}
    {{/nav-link-text}}
  {{else}}
    {{> @partial-block}}
  {{/if}}
  {{#ifAny nav-item--expandable nav-item--IsFlyout nav-item--IsDrilldown}}
    {{> nav-toggle}}
  {{/ifAny}}
</{{#if nav-item--expandable}}button{{else}}a{{/if}}>
