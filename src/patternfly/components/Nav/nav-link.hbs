<{{#if nav-item--expandable}}button{{else}}a href="{{nav-link--href}}"{{/if}} class="pf-c-nav__link{{#if nav-link--current}} pf-m-current{{/if}}{{#if nav-link--modifier}} {{nav-link--modifier}}{{/if}}"
  {{#if nav-link--attribute}}
    {{{nav-link--attribute}}}
  {{/if}}
  {{#if nav-link--current}}
    aria-current="page"
  {{/if}}
  {{#if nav-item--expandable}}
    {{#if nav-item--expanded}}
      aria-expanded="true"
    {{else}}
      aria-expanded="false"
    {{/if}}
  {{/if}}>
  {{> @partial-block}}
  {{#if nav-item--expandable}}
    {{> nav-toggle}}
  {{/if}}
</{{#if nav-item--expandable}}button{{else}}a{{/if}}>
