{{#> app-launcher id="app-launcher-example-sections-icons" app-launcher--IsExpanded="true" app-launcher--IsGrouped="true"}}
  {{#> app-launcher-menu}}
    {{#> app-launcher-group}}
      <ul>
        <li>
          {{#> app-launcher-menu-item app-launcher-menu-item--attribute='href="#"'}}
