{{#if checkNested}}
  <label class="pf-c-form__label {{#if disabled}}pf-m-disabled{{/if}}" for="{{ checkboxId }}">
    <input class="pf-c-form__checkbox" type="checkbox" id="{{ checkboxId }}" name="{{ checkboxName }}" value="{{ checkboxValue }}" {{#if disabled}} disabled {{/if}} {{#if checked}} checked {{/if}} {{#if tabindex }} tabindex="{{ tabindex }}" {{/if}} />
    {{checkboxLabel}}
  </label>
  {{> @partial-block }}
{{else}}
  <input class="pf-c-form__checkbox" type="checkbox" id="{{ checkboxId }}" name="{{ checkboxName }}" value="{{ checkboxValue }}" {{#if disabled}} disabled {{/if}} {{#if checked}} checked {{/if}} {{#if tabindex }} tabindex="{{ tabindex }}" {{/if}} />
  <label class="pf-c-form__label {{#if disabled}}pf-m-disabled{{/if}}" for="{{ checkboxId }}">{{checkboxLabel}}</label>
  {{> @partial-block }}
{{/if}}
