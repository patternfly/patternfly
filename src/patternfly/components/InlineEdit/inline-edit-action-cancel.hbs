{{#if inline-edit-action-cancel--IsIcon}}
  {{#> inline-edit-action}}
    {{#> button button--modifier="pf-m-plain" button--attribute=(concat 'aria-label="Cancel edits for row ' inline-edit-row '"')}}
      <i class="fas fa-times" aria-hidden="true"></i>
    {{/button}}
  {{/inline-edit-action}}
{{else}}
  {{#> inline-edit-action}}
    {{#> button button--modifier="pf-m-secondary"}}
      Cancel
    {{/button}}
  {{/inline-edit-action}}
{{/if}}
