{{#if toolbar-icon-button-group-example--IsOverflowMenu}}
  {{#> toolbar-item toolbar-item--modifier=(concat 'pf-m-overflow-menu ' toolbar-icon-button-group-example)}}
    {{#>overflow-menu
        overflow-menu-button--aria-label="Expand icon button overflow menu"
        overflow-menu--id=(dasherize toolbar--id 'icon-button-overflow-menu')
        overflow-menu--modifier=toolbar-demo-overflow-menu--modifier
        overflow-menu--attribute=toolbar-demo-overflow-menu--attribute
      }}

      {{#if toolbar-icon-button-group-example--control}}
        {{#> overflow-menu-content}}
          {{#> overflow-menu-group overflow-menu-group--modifier="pf-m-icon-button-group"}}
            {{#> overflow-menu-item}}
              {{> button button--IsPlain=true button--IsIcon=true button--icon="edit" button--aria-label="Edit"}}
            {{/overflow-menu-item}}
            {{#> overflow-menu-item}}
              {{> button button--IsPlain=true button--IsIcon=true button--icon="clone" button--aria-label="Clone"}}
            {{/overflow-menu-item}}
            {{#> overflow-menu-item}}
              {{> button button--IsPlain=true button--IsIcon=true button--icon="sync" button--aria-label="Sync"}}
            {{/overflow-menu-item}}
          {{/overflow-menu-group}}
        {{/overflow-menu-content}}
      {{/if}}
    {{/overflow-menu}}
  {{/toolbar-item}}
{{else}}
  {{#> toolbar-group toolbar-group--modifier="pf-m-icon-button-group"}}
    {{#> toolbar-item}}
      {{> button button--IsPlain=true button--IsIcon=true button--icon="edit" button--aria-label="Edit"}}
    {{/toolbar-item}}
    {{#> toolbar-item}}
      {{> button button--IsPlain=true button--IsIcon=true button--icon="clone" button--aria-label="Clone"}}
    {{/toolbar-item}}
    {{#> toolbar-item}}
      {{> button button--IsPlain=true button--IsIcon=true button--icon="sync" button--aria-label="Sync"}}
    {{/toolbar-item}}
  {{/toolbar-group}}
{{/if}}
