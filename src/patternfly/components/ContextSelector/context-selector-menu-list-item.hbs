<{{#if context-selector-menu-list-item--IsLink}}a{{else}}button{{/if}} class="pf-c-context-selector__menu-list-item
  {{~#if context-selector-menu-list-item--IsLink}}
    {{~#if context-selector-menu-list-item--IsDisabled}} pf-m-disabled{{/if}}
  {{/if}}
  {{~#if context-selector-menu-list-item--modifier}} {{context-selector-menu-list-item--modifier}}{{/if}}"
  {{#if context-selector-menu-list-item--IsLink}}
    href="#"
    {{#if context-selector-menu-list-item--IsDisabled}}
      aria-disabled="true"
      tabindex="-1"
    {{/if}}
  {{else}}
    type="button"
    {{#if context-selector-menu-list-item--IsDisabled}}
      disabled
    {{/if}}
  {{/if}}
  {{#if context-selector-menu-list-item--attribute}}
    {{{context-selector-menu-list-item--attribute}}}
  {{/if}}>
  {{> @partial-block}}
</{{#if context-selector-menu-list-item--IsLink}}a{{else}}button{{/if}}>
