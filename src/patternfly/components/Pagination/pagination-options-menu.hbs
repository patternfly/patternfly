{{#> options-menu options-menu--modifier=pagination-options-menu--modifier}}
  {{#> options-menu-toggle options-menu-toggle--type="div" options-menu-toggle--modifier="pf-m-plain"}}
    {{#if pagination--IsIndeterminate}}
      <span class="pf-c-options-menu__toggle-text"><b>1 - 10</b>&nbsp;of&nbsp;<b>many</b></span>
    {{else}}
      <span class="pf-c-options-menu__toggle-text"><b>1 - 10</b>&nbsp;of&nbsp;<b>36</b></span>
    {{/if}}
    {{> options-menu-toggle-button aria-label="Items per page"}}
  {{/options-menu-toggle}}
  {{#> options-menu-menu}}
    {{#> options-menu-menu-item}}
      5 per page
    {{/options-menu-menu-item}}
    {{#> options-menu-menu-item options-menu-menu-item--IsSelected="true"}}
      10 per page
    {{/options-menu-menu-item}}
    {{#> options-menu-menu-item}}
      20 per page
    {{/options-menu-menu-item}}
  {{/options-menu-menu}}
{{/options-menu}}
