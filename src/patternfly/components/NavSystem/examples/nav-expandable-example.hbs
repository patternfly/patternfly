            {{/nav-list-simple-link}}
          {{/nav-list-simple-item}}
        {{/nav-list-simple}}
      {{/nav-subnav}}
    {{/nav-list-item}}
    {{#> nav-list-item nav-list-item--expandable="true"}}
      {{#> nav-list-link nav-list-link--href="#" nav-list-link--attribute='id="link3"'}}
