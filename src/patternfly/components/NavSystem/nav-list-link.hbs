<a href="{{href}}" class="pf-c-nav__list{{#if nav-list--type}}-{{nav-list--type}}{{/if}}-link{{#if disabled}} pf-m-disabled{{/if}}{{#if nav-list-link--modifiers}} {{nav-list-link--modifiers}}{{/if}}"
  {{#if nav-list-link--attributes}}
    {{{nav-list-link--attributes}}}
  {{/if}}

  {{#if disabled}}
    aria-disabled="true"
    tabindex="-1" 
  {{/if}}

  {{#if expandable}}
    {{#unless subnav}}
      {{#if expanded}}
        aria-expanded="true"
      {{else}}
        aria-expanded="false"
      {{/if}}
    {{/unless}}
  {{/if}}>

  {{> @partial-block}}
  {{#if expandable}}
    {{#unless subnav}}
    <span class="pf-c-nav__list-expand">
      <i class="fas fa-chevron-right"></i>
    </span>
    {{/unless}}
  {{/if}}
</a>
