<{{#if dropdown-menu-item--type}}{{dropdown-menu-item--type}}{{else}}button{{/if}} class="{{pfv}}dropdown__menu-item{{#if dropdown-menu-item--modifier}} {{dropdown-menu-item--modifier}}{{/if}}{{#if dropdown-menu-item--HasIcon}} pf-m-icon{{/if}}"
  {{#unless dropdown--HasCheckboxes}}
    role="menuitem"
  {{/unless}}
  {{#unless dropdown-menu-item--type}}
    type="button"
  {{/unless}}
  {{#if dropdown-menu-item--attribute}}
    {{{dropdown-menu-item--attribute}}}
  {{/if}}>
  {{#if dropdown-menu-item--HasIcon}}
    {{#> dropdown-menu-item-icon}}
      {{#if dropdown-menu-item--IsPFicon}}
        <img src="/assets/images/pf-logo-small.svg" alt="PatternFly logo">
      {{else}}
        <i class="fas fa-{{#if dropdown-menu-item--icon}}{{dropdown-menu-item--icon}}{{else}}cog{{/if}}" aria-hidden="true"></i>
      {{/if}}
    {{/dropdown-menu-item-icon}}
  {{/if}}
  {{#if dropdown-menu-item--text}}
    {{{dropdown-menu-item--text}}}
  {{/if}}
  {{#if @partial-block}}
    {{> @partial-block}}
  {{/if}}
</{{#if dropdown-menu-item--type}}{{dropdown-menu-item--type}}{{else}}button{{/if}}>
