{{#> dropdown-menu}}
  {{> dropdown-menu-list-item dropdown-menu-item--text="Link" dropdown-menu-item--type="a" dropdown-menu-item--PFicon="true" dropdown-menu-item--attribute='href="#"' dropdown-menu-item--HasIcon=dropdown-menu--HasIcons dropdown-menu-item--IsPFicon="true"}}
  {{> dropdown-menu-list-item dropdown-menu-item--text="Action" dropdown-menu-item--HasIcon=dropdown-menu--HasIcons}}
  {{#if dropdown--IsAriaDisabled}}
    {{> dropdown-menu-list-item dropdown-menu-item--text="Disabled link" dropdown-menu-item--type="a" dropdown-menu-item--modifier="pf-m-aria-disabled" dropdown-menu-item--attribute='href="#" aria-disabled="true"'}}
    {{> dropdown-menu-list-item dropdown-menu-item--text="Disabled action" dropdown-menu-item--modifier="pf-m-aria-disabled" dropdown-menu-item--attribute='aria-disabled="true"'}}
  {{else}}
    {{> dropdown-menu-list-item dropdown-menu-item--text="Disabled link" dropdown-menu-item--type="a" dropdown-menu-item--modifier="pf-m-disabled" dropdown-menu-item--attribute='href="#" aria-disabled="true" tabindex="-1"'}}
    {{> dropdown-menu-list-item dropdown-menu-item--text="Disabled action" dropdown-menu-item--attribute="disabled"}}
  {{/if}}
  {{> divider divider--type="li"}}
  {{> dropdown-menu-list-item dropdown-menu-item--text="Separated link" dropdown-menu-item--type="a" dropdown-menu-item--attribute='href="#"'}}
{{/dropdown-menu}}
