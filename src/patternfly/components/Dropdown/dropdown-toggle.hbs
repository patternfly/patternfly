<{{# if dropdown-toggle--type}}{{dropdown-toggle--type}}{{else}}button{{/if}} class="pf-c-dropdown__toggle{{#if dropdown-toggle--modifier}} {{dropdown-toggle--modifier}}{{/if}}"
  {{#unless dropdown--isSplitButton}}
    id="{{id}}-button"
    aria-expanded="{{#if dropdown--IsExpanded}}true{{else}}false{{/if}}"
  {{/unless}}
  {{#if aria-label}}
    aria-label="{{aria-label}}"
  {{/if}}>
  {{> @partial-block}}
  {{#if dropdown--HasToggleIcon}}
    {{> dropdown-toggle-icon}}
  {{/if}}
  {{#if dropdown--HasKebabIcon}}
    {{> kebab-icon}}
  {{/if}}
</{{# if dropdown-toggle--type}}{{dropdown-toggle--type}}{{else}}button{{/if}}>