{{#> tabs-item tabs-item--IsCurrent=__tabs-item--current tabs-item--modifier=__tabs-item--modifier tabs-item--attribute=__tabs-item--attribute tabs-item--IsOverflow=__tabs-item--IsOverflow tabs-item--IsOverflowExpanded=__tabs-item--IsOverflowExpanded}}
  {{#> tabs-link tabs-link--attribute=(concat __tabs-link--attribute ' id="' tabs--id '-' __tabs-item--id '-link"') tabs-link--IsOverflowExpanded=__tabs-item--IsOverflowExpanded}}
    {{#if __tabs-list--HasIcons}}
      {{> tabs-item-icon tabs-item-icon--name=__tabs-item--icon-name}}
    {{/if}}
    {{#unless __tabs-item--NoText}}
      {{#> tabs-item-text}}
        {{{__tabs-item--text}}}
      {{/tabs-item-text}}
    {{/unless}}
    {{#if __tabs-item--IsOverflow}}
      {{> tabs-link-toggle-icon}}
    {{/if}}
  {{/tabs-link}}
{{/tabs-item}}
