<input class="{{pfv}}text-input-group__text-input
  {{~#if text-input-group-text-input--hint-text}} pf-m-hint{{/if}}
  {{~#if text-input-group-text-input--modifier}}
    {{text-input-group-text-input--modifier}}
  {{/if}}"
  type="{{#if text-input-group-text-input--type}}{{text-input-group-text-input--type}}{{else}}text{{/if}}"
  {{#if text-input-group-text-input--hint-text}}
    value="{{text-input-group-text-input--hint-text}}"
    disabled
    aria-hidden="true"
  {{else}}
    {{#if text-input-group-text-input--placeholder}}
      placeholder="{{text-input-group-text-input--placeholder}}"
    {{/if}}
    {{#if text-input-group--IsDisabled}}
      disabled
    {{/if}}
    value="
      {{~#if text-input-group--IsDisabled~}}
        Disabled
      {{~else~}}
        {{text-input-group--value}}
      {{~/if}}"
    aria-label="
      {{~#if text-input-group-text-input--aria-label~}}
        {{text-input-group-text-input--aria-label}}
      {{~else if text-input-group--IsDisabled~}}
        Disabled text input group example input
      {{~else~}}
        Type to filter
      {{~/if}}"
  {{/if}}
  {{#if text-input-group-text-input--attribute}}
    {{{text-input-group-text-input--attribute}}}
  {{/if}}>
