{{#> text-input-group}}
  {{#> text-input-group-main text-input-group-main--modifier="pf-m-icon"}}
    {{#> text-input-group-text}}
      {{> text-input-group-icon}}
      {{> text-input-group-text-input text-input-group-text-input--aria-label="Search input"}}
    {{/text-input-group-text}}
  {{/text-input-group-main}}
  {{#if text-input-group--value}}
    {{#> text-input-group-utilities}}
      {{#if text-input-group--search-input--count}}
        {{> badge badge--modifier="pf-m-read" badge--text=text-input-group--search-input--count}}
        {{#if text-input-group--search-input--IsNavigable}}
          {{#> text-input-group-group}}
            {{> button
              button--IsPlain=true
              button--IsIcon=true
              button--icon="angle-up fa-fw"
              button--IsDisabled=text-input-group--search-input--IsFirstMatch
              button--aria-label="Previous"}}
            {{> button
              button--IsPlain=true
              button--IsIcon=true
              button--icon="angle-down fa-fw"
              button--IsDisabled=text-input-group--search-input--IsLastMatch
              button--aria-label="Next"}}
          {{/text-input-group-group}}
        {{/if}}
      {{/if}}
      {{> button button--IsPlain=true button--IsIcon=true button--icon="times fa-fw" button--attribute='aria-label="Clear input"'}}
    {{/text-input-group-utilities}}
  {{/if}}
{{/text-input-group}}
