<{{#if masthead-logo--type}}{{masthead-logo--type}}{{else}}a{{/if}} class="{{pfv}}masthead__logo{{#if masthead-logo--modifier}} {{masthead-logo--modifier}}{{/if}}"
  {{#unless masthead-logo--type}}
    {{#if masthead-logo--href}}
      href="{{masthead-logo--href}}"
    {{else}}
      href="#"
    {{/if}}
  {{/unless}}
  {{#if masthead-logo--attribute}}
    {{{masthead-logo--attribute}}}
  {{/if}}>
  {{#if @partial-block}}
    {{> @partial-block}}
  {{else}}
    {{> masthead-image}}
  {{/if}}
</{{#if masthead-logo--type}}{{masthead-logo--type}}{{else}}a{{/if}}>
