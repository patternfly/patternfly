{{#each (exampleObject)}}
  {{#> example-component example-component--index=@index example-component--id=(concat 'example-component-' @index)}}
    {{#> example-component-title example-component--id=(concat 'testing-' @index)}}
      {{this.name}} Key: {{@index}}
    {{/example-component-title}}
    {{#> example-component-body}}
      Age: {{this.age}}
      Index: {{@index}}
    {{/example-component-body}}
    {{#> example-component-footer}}
      {{this.items.shoes}}
      {{this.items.bike}}
      {{this.items.url}}
    {{/example-component-footer}}
    {{{html}}}
  {{/example-component}}
{{/each}}
