{{#if brand--IsPicture}}
  <picture class="{{pfv}}brand pf-m-picture{{#if brand--modifier}} {{brand--modifier}}{{/if}}"
    {{#if brand--attribute}}
      {{{brand--attribute}}}
    {{/if}}>
    {{#if brand--img-url-on-md}}
      <source media="(min-width: 768px)" srcset="{{brand--img-url-on-md}}">
    {{/if}}
    {{#if brand--img-url}}
      <source srcset="{{brand--img-url}}">
    {{/if}}
    {{#if brand--img-url--base}}
      <img src="{{brand--img-url--base}}" alt="Fallback patternFly default logo">
    {{else}}
      <img src="/assets/images/pf_logo.svg" alt="Fallback patternFly default logo">
    {{/if}}
  </picture>
{{else}}
  <img class="{{pfv}}brand{{#if brand--modifier}} {{brand--modifier}}{{/if}}"
    {{#if brand--attribute}}
      {{{brand--attribute}}}
    {{/if}}>
{{/if}}
