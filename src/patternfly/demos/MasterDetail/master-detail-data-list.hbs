{{#> data-list data-list--id=(concat master-detail-template--id '-data-list') data-list--attribute='aria-label="Simple data list example"'}}
  {{#> data-list-item data-list-item--modifier="pf-m-selectable" data-list-item--attribute=(concat 'aria-labelledby="' data-list--id '-item1"')}}
    {{#> data-list-item-row}}
      {{#> data-list-item-content}}
        {{#> data-list-cell data-list-cell--modifier="pf-m-align-left"}}
          {{#> l-flex l-flex--modifier="pf-m-column"}}
            {{#> l-flex-item}}
              <p id='{{data-list--id}}-item1'>patternfly</p>
              <small>Working repo for PatternFly 4 <a href="">https://pf4.patternfly.org/</a></small>
            {{/l-flex-item}}
            {{#> l-flex newcontext}}
              {{#> l-flex-item}}
                <i class="fas fa-code-branch" aria-hidden="true"></i>
                10
              {{/l-flex-item}}
              {{#> l-flex-item}}
                <i class="fas fa-code" aria-hidden="true"></i>
                4
              {{/l-flex-item}}
              {{#> l-flex-item}}
                <i class="fas fa-cube" aria-hidden="true"></i>
                5
              {{/l-flex-item}}
              {{#> l-flex-item}}
                Updated 2 days ago
              {{/l-flex-item}}
            {{/l-flex}}
          {{/l-flex}}
        {{/data-list-cell}}
        {{#> data-list-cell data-list-cell--modifier="pf-m-align-right pf-m-no-fill"}}
          {{#> button button--modifier="pf-m-secondary"}}
            Secondary
          {{/button}}
          {{#> button button--modifier="pf-m-link"}}
            Link button
          {{/button}}
        {{/data-list-cell}}
      {{/data-list-item-content}}
    {{/data-list-item-row}}
  {{/data-list-item}}

  {{#if drawer-panel--IsOpen}}
