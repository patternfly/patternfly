{{#> page-header-tools}}
  {{#> toolbar}}
    {{#> toolbar-group pf-l-toolbar__group--modifier="pf-u-sr-only pf-u-visible-on-lg"}}
      {{#> toolbar-item}}
        {{#> button btnClass="pf-m-plain" btnAttributes='aria-label="Overflow actions"'}}
          <i class="fas fa-bell"></i>
        {{/button}}
      {{/toolbar-item}}
      {{#> toolbar-item}}
        {{#> button btnClass="pf-m-plain" btnAttributes='aria-label="Overflow actions"'}}
          <i class="fas fa-cog"></i>
        {{/button}}
      {{/toolbar-item}}
    {{/toolbar-group}}
    {{#> toolbar-group}}
      {{#> toolbar-item pf-l-toolbar__item--modifier="pf-u-hidden-on-lg pf-u-mr-0"}}
        {{#> button btnClass="pf-m-plain" btnAttributes='aria-label="Overflow actions"'}}
          <i class="fas fa-ellipsis-v"></i>
        {{/button}}
      {{/toolbar-item}}
      {{#> toolbar-item pf-l-toolbar__item--modifier="pf-u-sr-only pf-u-visible-on-md"}}
        {{#> dropdown pf-c-dropdown--Modifier="pf-m-plain" id="dropdown-example-collapsed" dropdown--attribute="pf-m-plain" pf-c-dropdown--HasToggleIcon="true"}}
          Kyle Baker
        {{/dropdown}}
      {{/toolbar-item}}
    {{/toolbar-group}}
  {{/toolbar}}  
  {{#> avatar avatar--src="/assets/images/img_avatar.jpg" avatar--alt="Avatar image"}}{{/avatar}}
{{/page-header-tools}}