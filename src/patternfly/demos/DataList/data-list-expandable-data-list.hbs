{{#> data-list data-list--id=(concat page--id '-data-list') data-list--attribute='aria-label="Data list expandable demo"'}}
  {{#> data-list-item data-list-item--id="item-1" data-list-item--expanded="true"}}
    {{#> data-list-item-row}}
      {{#> data-list-item-control}}
        {{#> data-list-toggle button--attribute=(concat 'aria-labelledby="ex-toggle1 ' data-list--id '-item-1" id="ex-toggle1" aria-label="Toggle details for" aria-expanded="false" aria-controls="content-1"')}}{{/data-list-toggle}}
        {{#> data-list-check data-list-check-checkbox--attribute=(concat 'type="checkbox" name="check-expandable-check1" aria-labelledby="' data-list--id '-item-1"')}}{{/data-list-check}}
      {{/data-list-item-control}}
      {{#> data-list-item-content}}
