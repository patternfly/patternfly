                {{/nav-link}}
              {{/nav-item}}
              {{#> nav-item newcontent}}
                {{#> nav-link nav-link--href="#"}}
                  Subnav link 2
                {{/nav-link}}
              {{/nav-item}}
            {{/nav-list}}
          {{/nav-subnav}}
        {{/nav-item}}
      {{/nav-list}}
    {{/nav}}
  {{/page-sidebar}}
  {{#> page-main}}
    {{#> page-main-nav}}
