  {{#> card card--id=(concat card-template-gallery--id '-card-4') card--modifier="pf-m-selectable-raised pf-m-compact"}}
    {{#> card-header}}
      <img src="/assets/images/camel-avro_200x150.png" width="60px" alt="Logo">
