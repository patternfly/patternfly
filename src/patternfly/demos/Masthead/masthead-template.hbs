{{!--
  Options:
  masthead-template-content-toolbar-content: set custom toolbar content
--}}

{{#unless masthead-template--HasNoSkipToContent}}
  {{> page-template-skip-to-content}}
{{/unless}}
{{#> masthead masthead--id=masthead-template--id masthead--modifier=masthead-template--modifier masthead-template-content-icon-group--breakpoint="lg"}} {{!-- set default breakpoint --}}
  {{#unless masthead-template--HasNoToggle}}
    {{> masthead-toggle}}
  {{/unless}}
  {{#> masthead-main}}
    {{> masthead-brand}}
  {{/masthead-main}}
  {{> masthead-template-content}}
{{/masthead}}
