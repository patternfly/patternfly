{{!--
  Options:
  masthead-template-content-toolbar-content: set custom toolbar content
--}}

{{#unless masthead-template--HasNoSkipToContent}}
  {{> page-template-skip-to-content}}
{{/unless}}
{{#> masthead masthead--id=masthead-template--id masthead--modifier=masthead-template--modifier masthead-template-content-icon-group--breakpoint="lg"}} {{!-- set default breakpoint --}}
  {{#unless masthead-template--HasNoToggle}}
    {{> masthead-toggle}}
  {{/unless}}
  {{#> masthead-main}}
    {{#> masthead-brand}}
      {{> brand
        brand--attribute='style="--pf-c-brand--Width: 180px; --pf-c-brand--Width-on-md: 180px; --pf-c-brand--Width-on-2xl: 220px;"'
        brand--IsPicture="true"
        brand--img-url--base='/assets/images/logo__pf--reverse--base.png'
        brand--img-url='/assets/images/logo__pf--reverse--base.svg'
        brand--img-url-on-md='/assets/images/logo__pf--reverse-on-md.svg'}}
    {{/masthead-brand}}
  {{/masthead-main}}
  {{> masthead-template-content}}
{{/masthead}}
