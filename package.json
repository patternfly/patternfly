    "gatsby-link": "^1.6.21",
    "gatsby-plugin-netlify-cms": "^1.0.2",
    "gatsby-plugin-pathdata": "^1.0.3",
    "gatsby-plugin-postcss-sass": "^1.0.16",
    "gatsby-plugin-react-helmet": "^1.0.5",
