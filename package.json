    "react-helmet": "^5.2.0",
    "react-ionicons": "^2.1.6",
    "react-load-script": "0.0.6",
    "react-prism": "^4.3.2",
    "rehype-react": "^3.0.2",
    "scaffold-helper": "^1.0.0",
    "stylelint": "^8.4.0",
