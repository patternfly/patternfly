{
  "name": "@patternfly/patternfly-next",
  "description": "Assets, source, tooling, and content for PatternFly Next",
  "version": "0.0.0-development",
  "author": "Andres Galante",
  "keywords": [],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "backstop:approve": "node test/approve-conflicts.js",
    "backstop:init": "node test/create-references.js",
    "bootstrap:build": "node npm-scripts/build-bootstrap-workspace.js",
    "build-patternfly": "rm -rf dist && rm -rf tmp && gulp build",
    "postinstall": "npm run build:icons && gulp copy-icons",
    "build": "gatsby build",
    "build:icons": "node ./src/icons/generateIcons.js",
    "build-ghpages": "gatsby build --prefix-paths",
    "cli:setup": "cd build/patternfly-cli && npm install && cd ../../ && npm link ./build/patternfly-cli",
    "dev": "gatsby develop",
    "develop": "gatsby develop",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write src/**/*.js",
    "lint": "npm run lint:styles && npm run lint:js",
    "lint:fix": "npm run lint:js --fix && npm run lint:styles --fix",
    "lint:styles": "stylelint 'src/**/*.scss'",
    "lint:js": "eslint .",
    "precommit": "lint-staged",
    "sassdoc": "sassdoc ./src/patternfly/**/*.scss --parse > sassdoc/data.json",
    "serve": "gatsby serve",
    "start": "npm run develop",
    "test": "node test/test.js",
    "gen:sitemap": "node ./generateSitemap",
    "audit:a11y": "node ./test-a11y/",
    "a11y": "npm run gen:sitemap && npm run audit:a11y",
    "test:ci": "start-server-and-test dev \"http://localhost:8000\" a11y",
    "update-modifiers": "./build/extractModifiers.sh",
    "semantic-release": "semantic-release"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.scss": [
      "stylelint --fix",
      "git add"
    ]
  },
  "dependencies": {},
  "devDependencies": {
    "@fortawesome/fontawesome": "^1.1.8",
    "axe-core": "^3.0.3",
    "axe-webdriverjs": "^2.0.1",
    "axios": "^0.18.0",
    "babel-eslint": "^8.2.3",
    "backstopjs": "^3.5.2",
    "commander": "^2.13.0",
    "del": "^3.0.0",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-config-standard-jsx": "^5.0.0",
    "eslint-config-standard-react": "^6.0.0",
    "eslint-import-resolver-alias": "^1.0.3",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "fs-extra": "^5.0.0",
    "gatsby": "^1.9.63",
    "gatsby-cli": "^1.1.40",
    "gatsby-link": "^1.6.21",
    "gatsby-plugin-postcss-sass": "^1.0.16",
    "gatsby-plugin-react-helmet": "^1.0.5",
    "gatsby-plugin-sharp": "^1.6.18",
    "gatsby-remark-autolink-headers": "^1.4.11",
    "gatsby-remark-embed-snippet": "^1.0.6",
    "gatsby-remark-prismjs": "^1.2.12",
    "gatsby-source-filesystem": "^1.5.5",
    "gatsby-transformer-json": "^1.0.14",
    "gatsby-transformer-remark": "^1.7.31",
    "gatsby-transformer-sharp": "^1.6.13",
    "gatsby-transformer-yaml": "^1.5.15",
    "glob": "^7.1.2",
    "gulp": "^3.9.1",
    "gulp-cssnano": "^2.1.3",
    "gulp-rename": "^1.2.3",
    "gulp-sass": "^3.1.0",
    "gulp-sass-glob": "^1.0.8",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-string-replace": "^1.0.0",
    "handlebars-loader": "^1.7.0",
    "html-loader": "^0.5.5",
    "husky": "^0.14.3",
    "inflection": "^1.12.0",
    "jsdom": "^11.8.0",
    "lint-staged": "^7.0.4",
    "lodash": "^4.17.5",
    "markdown-loader": "^2.0.2",
    "minimist": "^1.2.0",
    "node-plop": "^0.15.0",
    "postcss-cssnext": "^3.1.0",
    "precss": "^3.1.0",
    "prettier": "^1.12.1",
    "pretty": "^2.0.0",
    "prismjs": "^1.11.0",
    "prop-types": "^15.6.0",
    "raw-loader": "^0.5.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-helmet": "^5.2.0",
    "react-ionicons": "^2.1.6",
    "react-load-script": "0.0.6",
    "react-prism": "^4.3.2",
    "rehype-react": "^3.0.2",
    "scaffold-helper": "^1.0.0",
    "selenium-webdriver": "^4.0.0-alpha.1",
    "semantic-release": "^15.4.1",
    "start-server-and-test": "^1.5.0",
    "stylelint": "^9.4.0",
    "stylelint-config-recommended-scss": "^3.0.0",
    "stylelint-config-standard": "^18.0.0",
    "stylelint-order": "^0.8.0",
    "stylelint-scss": "^2.5.0",
    "stylelint-webpack-plugin": "^0.10.2",
    "webpack-notifier": "^1.5.1"
  },
  "engines": {
    "node": ">=8.0.0",
    "npm": ">=5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/patternfly/patternfly-next.git"
  },
  "release": {
    "branch": "master",
    "analyzeCommits": {
      "preset": "angular",
      "releaseRules": [
        {
          "type": "breaking",
          "release": "patch"
        },
        {
          "type": "feat",
          "release": "patch"
        },
        {
          "type": "fix",
          "release": "patch"
        },
        {
          "type": "perf",
          "release": "patch"
        },
        {
          "type": "docs",
          "release": "patch"
        },
        {
          "type": "refactor",
          "release": "patch"
        }
      ]
    },
    "prepare": [
      {
        "path": "@semantic-release/npm",
        "pkgRoot": "dist"
      }
    ],
    "publish": [
      {
        "path": "@semantic-release/npm",
        "pkgRoot": "dist"
      },
      "@semantic-release/github"
    ]
  },
  "resolutions": {
    "node-sass": "4.7.2"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "tag": "latest"
  }
}
